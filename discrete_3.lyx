\begin_layout Standard
\begin_inset Formula 
\begin{align*}
f\left(x,y,z\right) & =\overline{x}\cdot\overline{y}\cdot\overline{z}\lor\overline{x}\cdot\overline{y}\cdot z\lor\overline{x}\cdot y\cdot\overline{z}\lor x\cdot\overline{y}\cdot\overline{z}\lor x\cdot\overline{y}\cdot z\lor x\cdot y\cdot\overline{z}\lor x\cdot y\cdot z\\
 & =\overline{x}\cdot\overline{y}\cdot\left(\overline{z}\lor z\right)\lor x\cdot\overline{z}\cdot\left(\overline{y}\lor y\right)\lor\overline{x}\cdot y\cdot\overline{z}\lor x\cdot z\cdot\left(\overline{y}\lor y\right)\\
 & =\overline{x}\cdot\overline{y}\lor x\cdot\overline{z}\lor\overline{x}\cdot y\cdot\overline{z}\lor x\cdot z\\
 & =x\cdot\left(\overline{z}\lor z\right)\lor\overline{x}\cdot\overline{y}\lor\overline{x}\cdot y\cdot\overline{z}\\
 & =x\lor\overline{x}\cdot\overline{y}\lor\overline{x}\cdot y\cdot\overline{z}
\end{align*}

\end_inset


\end_layout

